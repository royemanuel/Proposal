@article{Miller-Hooks2012,
abstract = {In assessing a networks potential performance given possible future disruptions, one must recognize the contributions of the networks inherent ability to cope with disruption via its topological and operational attributes and potential actions that can be taken in the immediate aftermath of such an event. Measurement and maximization of network resilience that accounts for both in the context of intermodal freight transport are addressed herein. That is, the problem of measuring a networks maximum resilience level and simultaneously determining the optimal set of preparedness and recovery actions necessary to achieve this level under budget and level-of-service constraints is formulated as a two-stage stochastic program. An exact methodology, employing the integer L-shaped method and Monte Carlo simulation, is proposed for its solution. Optimal allocation of a limited budget between preparedness and recovery activities is explored on an illustrative problem instance involving a network abstraction of a United States rail-based intermodal container network. ?? 2011 Elsevier Ltd. All rights reserved.},
author = {Miller-Hooks, Elise and Zhang, Xiaodong and Faturechi, Reza},
doi = {10.1016/j.cor.2011.09.017},
file = {:D$\backslash$:/Mendeley/Miller-Hooks, Zhang, Faturechi - 2012 - Measuring and maximizing resilience of freight transportation networks.pdf:pdf},
isbn = {0305-0548},
issn = {03050548},
journal = {Computers and Operations Research},
keywords = {Emergency preparedness,Flexibility,Recourse,Recovery,Reliability,Robustness,Stochastic programming,Vulnerability},
number = {7},
pages = {1633--1643},
publisher = {Elsevier},
title = {{Measuring and maximizing resilience of freight transportation networks}},
url = {http://dx.doi.org/10.1016/j.cor.2011.09.017},
volume = {39},
year = {2012}
}
