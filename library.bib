pages = {2327--36},
pmid = {21295166},
publisher = {Acta Materialia Inc.},
title = {{Osteoinduction of porous Ti implants with a channel structure fabricated by selective laser melting.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21295166},
volume = {7},
year = {2011}
}
