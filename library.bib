journal = {Risk Analysis},
keywords = {Decomposition,Demand reduction model,Infrastructure inoperability,Interdependency analysis,Leontief input-output model},
number = {6},
pages = {1437--1451},
pmid = {15660602},
title = {{Modeling the demand reduction input-output (I-O) inoperability due to terrorism of interconnected infrastructures}},
volume = {24},
