mendeley-tags = {trauma},
month = {apr},
number = {4},
pages = {1640--8},
pmid = {19913643},
publisher = {Acta Materialia Inc.},
title = {{Influence of porosity on mechanical properties and in vivo response of Ti6Al4V implants.}},
