keywords = {*cancer diagnosis,*diagnostic procedure,*health care personnel,*medical practice,article,colorectal cancer,decision making,diagnostic test,follow up,health care planning,human,lung cancer,patient compliance,patient referral,physician attitude,primary medical care},
