journal = {IEEE Transactions on Components and Packaging Technologies},
keywords = {Bayesian decision network,Obsolescence,Sustainment,Technology insertion,Viability},
number = {4},
pages = {734--738},
title = {{Optimum technology insertion into systems based on the assessment of viability}},
volume = {26},
year = {2003}
}
