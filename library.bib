keywords = {Deterrence,Optimization,Port security grants,Risk analysis,Supply chain resilience,Systems engineering},
number = {2},
pages = {521--554},
title = {{Options and challenges of a resilience-based, network-focused port security grant program}},
volume = {10},
year = {2013}
}
