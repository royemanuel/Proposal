@article{Clare2010,
abstract = {This article presents the design, construction, and test results of a linear ion trap, fabricated using digital light processing (DLP), which is a low-cost 3D layer-based manufacturing technique. The ion trap was incorporated into a portable mass spectrometer system and experimental mass spectrum was obtained for methamphetamine (m/z 182), cocaine (m/z 304), and rhodamine B (m/z 443), with a maximum observed resolution (FWHM) of 260. For rhodamine B, tandem MS capability is also demonstrated. The mass range (and resolution at higher m/z) of the instrument is also demonstrated by spectrum obtained from Ultramark (m/z 1621). The spectra obtained for the DLP trap occur at a considerably lower rf voltage than a rectilinear ion trap of similar size, which is a consequence of the hyperbolic electrode geometry and, hence, smaller r(0) in the DLP case. High mass range with low voltage operation is especially important with regard to 'in the field' applications requiring low power consumption for extended periods of operation.},
author = {Clare, Adam T and Gao, Liang and Brki{\'{c}}, Boris and Chalker, Paul R and Taylor, Stephen},
doi = {10.1016/j.jasms.2009.10.020},
file = {::},
