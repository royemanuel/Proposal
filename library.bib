journal = {Reliability Engineering and System Safety},
keywords = {Net present value (NPV),Optimal reliability,Revenue generation,Value of reliability},
number = {6},
pages = {665--673},
title = {{Reliability: How much is it worth? Beyond its estimation or prediction, the (net) present value of reliability}},
