isbn = {0951-8320},
issn = {09518320},
journal = {Reliability Engineering and System Safety},
keywords = {AGREE report,Development of reliability engineering,History of reliability,Increased specialization,System focus,history},
mendeley-tags = {history},
number = {2},
pages = {249--256},
title = {{Highlights from the early (and pre-) history of reliability engineering}},
