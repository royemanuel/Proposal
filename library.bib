@article{Cirillo2015,
abstract = {Statistical analyses on actual data depict operational risk as an extremely heavy-tailed phenomenon, able to generate losses so extreme as to suggest the use of infinite-mean models. But no loss can actually destroy more than the entire value of a bank or of a company, and this upper bound should be taken into consideration when dealing with tail-risk assessment. In financial risk management no risk can really be infinite, given that it always exists an upper bound, no matter how big. Introducing what we call the dual distribution, we show how to deal with heavy-tailed phenomena with a remote yet finite upper bound. We show how to compute relevant tail quantities like the Value-at-Risk (V aR) and the Expected Shortfall (ES). Our methodology is useful with apparently infinite-mean phenomena, as in the case of operational risk, but it can be applied in all those situations involving fat-tails and bounded supports.},
author = {Cirillo, Pasquale and Taleb, Nassim N},
file = {:D$\backslash$:/Mendeley/Cirillo, Taleb - 2015 - Expected shortfall estimation for apparently infinite-mean models of operational risk.pdf:pdf},
keywords = {Dismal Theorem,Dual distribution,Expected Shortfall,Fat-tails,Operational Risk,Upper bound,Value-at-risk},
pages = {1--14},
title = {{Expected shortfall estimation for apparently infinite-mean models of operational risk}},
year = {2015}
}
@article{Kontogiannis2013,
abstract = {The continued growth of civil aviation and the introduction of new air traffic management systems have increased the complexity of the system, hence requiring more adaptable patterns of control and coordination. There is a need to look deeper into the patterns of control, the transfer of control and coordination across boundaries or sectors and the adaptation to unexpected scenarios that may contribute to ‘loss of control’ events. To this end, this article elaborates on the extended control model (Hollnagel and Woods 2005) and complements a functional description of the air traffic control (ATC) system with several strategies required in ‘being in control’ of the joint cognitive system. Reviews of the literature, field studies and observations of performance in ATC have been used to model ‘loss of control’ events as problems in adapting control strategies in order to (1) maintain control of actions, (2) transfer control, (3) coordinate and (4) choose new modes of functioning and recovery when control breaks down. These control aspects are useful for debriefing controllers after critical events and identifying system performance flaws that can provide input to ergonomic interventions, such as the design of new air traffic management systems, decision aids and role allocation.},
author = {Kontogiannis, Tom and Malakis, Stathis},
doi = {10.1007/s10111-011-0209-0},
file = {:D$\backslash$:/Mendeley/Kontogiannis, Malakis - 2013 - Strategies in controlling, coordinating and adapting performance in air traffic control Modelling 'loss o.pdf:pdf},
issn = {14355558},
journal = {Cognition, Technology and Work},
keywords = {Adaptation,Air traffic control,Control strategies,Coordination,Extended control model (ECOM),Loss of control events},
number = {2},
pages = {153--169},
title = {{Strategies in controlling, coordinating and adapting performance in air traffic control: Modelling 'loss of control' events}},
volume = {15},
year = {2013}
}
